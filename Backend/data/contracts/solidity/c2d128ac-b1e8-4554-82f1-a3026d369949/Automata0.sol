pragma solidity ^0.8.0;contract Automata0 {enum State {INITIAL,COMPLETED}State currentState = State.INITIAL;function completed_to_initial() public {require(currentState == State.COMPLETED);bool transitionExecuted = false;if (currentState == State.COMPLETED) {currentState = State.INITIAL;transitionExecuted = true;}require(transitionExecuted, "No valid transition conditions met");}function get_current_state() public view returns (uint) { return uint(currentState); }function is_completed() public view returns (bool) { return currentState == State.COMPLETED; }function to_completed() public {currentState = State.COMPLETED;}}