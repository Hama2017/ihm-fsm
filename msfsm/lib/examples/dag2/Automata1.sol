pragma solidity ^0.8.0;contract Automata1 {enum State {Q1,Q2,COMPLETED}State currentState = State.Q1;function valider() public {require(currentState == State.Q1);bool transitionExecuted = false;if (currentState == State.Q1) {currentState = State.Q2;transitionExecuted = true;}require(transitionExecuted, "No valid transition conditions met");}function reporter() public {require(currentState == State.Q2);bool transitionExecuted = false;if (currentState == State.Q2) {currentState = State.COMPLETED;transitionExecuted = true;}require(transitionExecuted, "No valid transition conditions met");}function get_current_state() public view returns (uint) { return uint(currentState); }function is_completed() public view returns (bool) { return currentState == State.COMPLETED; }}