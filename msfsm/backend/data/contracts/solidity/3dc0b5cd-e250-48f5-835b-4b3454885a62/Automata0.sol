pragma solidity ^0.8.0;contract Automata0 {enum State {INITIAL,COMPLETED}State currentState = State.INITIAL;function refuser() public {require(currentState == State.INITIAL);bool transitionExecuted = false;if (currentState == State.INITIAL) {currentState = State.COMPLETED;transitionExecuted = true;}require(transitionExecuted, "No valid transition conditions met");}function get_current_state() public view returns (uint) { return uint(currentState); }function is_completed() public view returns (bool) { return currentState == State.COMPLETED; }}